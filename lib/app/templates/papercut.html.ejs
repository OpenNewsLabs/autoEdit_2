
<div class="row papercut" draggable="true" 
data-start-time			="<%= papercut[0].startTime %>"  
data-end-time			="<%= papercut[papercut.length -1].endTime %>" 
data-transcription-id	="<%= papercut[0].transcriptionId %>" 
data-reel-name			="<%= papercut[0].reelName %>" 
data-fps				="<%= papercut[0].fps %>" 
data-clip-name			="<%= papercut[0].clipName %>" 
data-video-id			="<%= papercut[0].videoId %>" 
data-speaker			="<%= papercut[0].speaker %>" 
data-src				="<%= papercut[0].src %>" 
data-audio-file			="<%= papercut[0].audioFile %>" 
data-offset				="<%= papercut[0].offset %>" 
>
	<dl class="dl-horizontal">
	<!-- TODO: once added speaker diarization this needs to be changes. cannot hardcode the paragraph-->
		<dt class="speaker"><%= papercut[0].speaker %></dt> 
		<dt>
	  		<a data-start-time="<%= papercut[0].startTime %>" data-video-id="<%= papercut[0].videoId %>" class="timecodes"><%= fromSeconds(papercut[0].startTime) %></a>
		</dt>
		<dd>
			<% for(var i=0; i<papercut.length; i++){%>
			 	<span contenteditable ="false" class="words text-muted papereditWords" 
			 	data-transcription-id	="<%= papercut[i].transcriptionId %>" 
			 	data-reel-name			="<%= papercut[i].reelName %>" 
			 	data-fps				="<%= papercut[i].fps %>" 
			 	data-clip-name			="<%= papercut[i].clipName %>" 
			 	data-video-id			="<%= papercut[i].videoId %>" 
			 	data-speaker			="<%= papercut[i].speaker %>" 
			 	data-src				="<%= papercut[i].src %>" 
			 	data-audio-file			="<%= papercut[i].audioFile %>" 
			 	data-start-time			="<%= papercut[i].startTime %>" 
			 	data-text				="<%= papercut[i].text %>" 
			 	data-end-time			="<%= papercut[i].endTime %>">
			 	<%= papercut[i].text %> </span>
	 		<% } %>
	 	</dd>
	</dl>
</div>